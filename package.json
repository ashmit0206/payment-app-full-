{
  "name": "Grind",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start-user-app": "cd apps/user-app && next start",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "next-auth": "^4.24.13"
  }
}
